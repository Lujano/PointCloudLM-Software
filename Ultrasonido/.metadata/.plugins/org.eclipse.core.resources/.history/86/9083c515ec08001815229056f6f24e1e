void TI1_OnInterrupt(void)
{
  /* Write your code here ... */
		
	if (estado == ESPERAR){
	}
	else if (estado == BAJO){
		Bit1_SetVal();
		estado == ALTO;
		}
	else if (estado == ALTO){
		Bit1_ClrVal();
		estado == ESPERAR;
		}
}


